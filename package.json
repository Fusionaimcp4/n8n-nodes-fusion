{
  "name": "n8n-nodes-fusion",
  "version": "0.2.8",
  "description": "n8n community node for Fusion AI (NeuroSwitch multi-provider orchestration)",
  "keywords": [
    "n8n-community-node",
    "n8n",
    "fusion",
    "ai",
    "neuroswitch",
    "openai",
    "anthropic",
    "claude",
    "gemini",
    "orchestration",
    "langchain"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Fusionaimcp4/n8n-nodes-fusion",
  "author": {
    "name": "Fusion AI",
    "email": "support@mcp4.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fusionaimcp4/n8n-nodes-fusion.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/nodes/Fusion/FusionChatModel.node.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "copy nodes\\Fusion\\fusion.svg dist\\nodes\\Fusion\\fusion.svg && copy nodes\\Fusion\\Fusion.node.json dist\\nodes\\Fusion\\Fusion.node.json && copy nodes\\Fusion\\FusionChatModel.node.json dist\\nodes\\Fusion\\FusionChatModel.node.json && copy nodes\\Fusion\\FusionChat.node.json dist\\nodes\\Fusion\\FusionChat.node.json",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest",
    "audit": "npm audit --audit-level=moderate"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.txt"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/nodes/Fusion/FusionApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Fusion/FusionChatModel.node.js",
      "dist/nodes/Fusion/FusionChat.node.js",
      "dist/nodes/Fusion/Fusion.node.js"
    ]
  },
  "dependencies": {
    "@langchain/core": "^0.2.0",
    "langchain": "^0.2.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/node": "18.16.16",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "29.7.0",
    "n8n-core": "1.112.1",
    "n8n-workflow": "1.110.0",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@langchain/core": "^0.2.0",
    "langchain": "^0.2.0",
    "n8n-core": "*",
    "n8n-workflow": "*"
  },
  "peerDependenciesMeta": {
    "@langchain/core": {
      "optional": true
    },
    "langchain": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
